{
    "name": "Flyff Ext",
    "version": "1.00",
    "description": "Flyff Ext",
    "manifest_version": 3,
    "content_scripts": [
        {
            "js": ["init.js"],
            "matches": ["https://universe.flyff.com/*"],
            "all_frames": true,
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [{
        "matches": ["https://universe.flyff.com/*"],
        "resources": ["src/*"]
    }],
    "permissions": ["activeTab", "storage"]
}
